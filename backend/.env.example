# Puerto del servidor
# FARKLE_PORT=8080

# Tamaño del buffer de envío por WebSocket
# FARKLE_SEND_BUFFER_SIZE=256

# Número de jugadores por partida
# FARKLE_NUM_PLAYERS=2

# Número de dados
# FARKLE_NUM_DICE=6

# Longitud del código de partida
# FARKLE_GAME_CODE_LENGTH=5

# Caracteres permitidos en el código de partida
# FARKLE_GAME_CODE_CHARS=ABCDEFGHJKLMNPQRSTUVWXYZ23456789

# Puntuación por defecto para ganar
# FARKLE_DEFAULT_VICTORY_SCORE=2000

# Límites de puntuación para ganar (min/max)
# FARKLE_MIN_VICTORY_SCORE=100
# FARKLE_MAX_VICTORY_SCORE=100000

# Tiempo que se mantienen partidas terminadas antes de eliminarlas
# FARKLE_FINISHED_GAME_RETENTION=5m

# Intervalo de limpieza de partidas terminadas
# FARKLE_CLEANUP_INTERVAL=1m
